class  s145
{
	public static void main(String[] args) 
	{
		int row=8;
		int col=8;
		for(int i=0;i<row;i++)
		{
			for(int j=0;j<col;j++)
			{
				if(i==0||i==row-1||j==0||j==col-1||i==row/2||j==col/2)
				{
					System.out.print('*');
				}
				else
				{
					System.out.print(' ');
				}
			}
			System.out.println();
		
		}
	}
}
